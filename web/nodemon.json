{
  "restartable": "rs",
  "ignore": [
    "test",
    ".git",
    "node_modules/**/node_modules"
  ],
  "verbose": true,
  "execMap": {
    "js": "node "
  },
  "events": {
    "restart": "osascript -e 'display notification \"App restarted due to:\n'$FILENAME'\" with title \"nodemon\"'"
  },
  "watch": [
    "server",
    "./server.js",
    "./webpack.config.development.js"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js json"
}
